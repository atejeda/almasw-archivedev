  <LoggingConfig>
    <log:_ Name="Manager" minLogLevel="4" minLogLevelLocal="4" />
    <log:_ Name="jacorb@Manager" minLogLevel="5" minLogLevelLocal="5" />
  </LoggingConfig>
</Manager>
