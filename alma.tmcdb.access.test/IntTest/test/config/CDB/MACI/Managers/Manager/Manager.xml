  <ServiceComponents>
    <cdb:_ string="Log"/>
    <cdb:_ string="LogFactory"/>
    <cdb:_ string="NotifyEventChannelFactory"/>
    <cdb:_ string="ArchivingChannel"/>
    <cdb:_ string="LoggingChannel"/>
